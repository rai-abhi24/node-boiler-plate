        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-validator": "^7.2.1",
        "helmet": "^8.0.0",
        "http-status-codes": "^2.3.0",
        "joi": "^17.13.3",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.12.0",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "nodemon": "^3.1.9",
        "prettier": "3.4.2"
    }
