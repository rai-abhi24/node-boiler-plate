{
    "name": "boiler-plate",
    "description": "A boiler plate code for node js",
    "version": "1.0.0",
    "main": "src/server.js",
    "author": "Abhishek Rai",
    "license": "ISC",
    "scripts": {
        "start": "cross-env NODE_ENV=production node src/server.js",
        "dev": "cross-env NODE_ENV=development nodemon src/server.js",
